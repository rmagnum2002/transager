$('.Magazin_Chisinau, .Magazin_Comrat, .Magazin_Balti, .Magazin_Filial_Hincesti_142, .Magazin_Taraclia').text($('#partsModal').data('empty-stock'))
$('#partPrice').text('<%= @part_details["price"] %> MDL')
<% @part_details["stock"].each_with_index do |p| %>
  $(".<%= p[0] %>").text("<%= p[1] %>")
<% end %>
$('#partTitle').text('<%= @part.name %>')
$('.license_number').text('<%= @part.seller_id %>')
$('.original_number').text('<%= @part.manufacturer_id %>')
$('#partsModal').modal('show')
$('.loading').addClass('hidden')
